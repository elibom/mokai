<?xml version="1.0" encoding="UTF-8"?>

<receivers xmlns="http://mokai.googlecode.com/receivers" 
		   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		   xsi:schemaLocation=
			"http://mokai.googlecode.com/receivers http://mokai.googlecode.com/svn/schema/receiver-0.1.xsd">
			
  <receiver id="test-1" className="org.mokai.types.mock.MockConnector" >
  	<post-receiving-actions>
  		<action className="org.mokai.types.mock.MockAction" />
  	</post-receiving-actions>
  </receiver>
  
  <receiver id="test-2" className="org.mokai.types.mock.MockConfigurableConnector">
  	<configuration>
  		<property name="config1">test</property>
  		<property name="config2">2</property>
  	</configuration>
  	<post-receiving-actions>
  		<action className="org.mokai.types.mock.MockConfigurableAction">
  			<property name="config1">test</property>
  			<property name="config2">2</property>
  		</action>
  	</post-receiving-actions>
  </receiver>
</receivers>